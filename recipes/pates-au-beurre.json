{
  "author": "Théo Petit",
  "date": "31/08/2022",
  "name": "Pâtes au beurre",
  "type": "Plat",
  "serves": 1,
  "ingredients": [
    {
      "Pâtes": {
        "quantity": "150g"
      }
      
    },
    {
      "Beurre salé": {
        "quantity": "20g"
      }
      
    }
  ],
  "instructions": [
    "Faire bouillir de l'eau.",
    "Cuire puis égoutter les pâtes.",
    "Ajouter le beurre salé et mélangez.",
    "Servir chaud."
  ]
}